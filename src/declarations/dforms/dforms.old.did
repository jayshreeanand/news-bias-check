type Phone = text;
type NewsSource = 
 record {
   accuracy: text;
   bias: text;
   domain: text;
   source: text;
 };
type Name = text;
type Entry = 
 record {
   desc: text;
   phone: Phone;
 };
service : {
  greet: (text) -> (text);
  insert: (text, text, text, text) -> ();
  insertOld: (Name, Entry) -> ();
  lookup: (text) -> (opt NewsSource) query;
  lookupOld: (Name) -> (opt Entry) query;
}
